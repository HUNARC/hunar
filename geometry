<html>
    <body>
        <canvas width="1000px" height="1000px" id="canvas"></canvas>
    </body>
    <script>
        var canvas=document.getElementById("canvas")
        var context=canvas.getContext("2d")
        context.fillStyle="orange"
        context.fillRect(100,150,250,250)

        context.fillStyle="red"
        context.fillRect(150,200,250,250)

        context.strokeStyle="blue"
        context.strokeRect(200,250,250,250)

        context.strokeStyle="green"
        context.strokeRect(250,300,250,250)

        context.strokeStyle="yellow"
        context.strokeRect(300,350,250,250)

        context.strokeStyle="black"
        context.strokeRect(350,400,250,250)

        context.beginPath()
        context.arc(600,275,50,1.5*Math.PI,1*Math.PI,true)
        context.fill()

        context.beginPath()
        context.arc(650,275,50,1.5*Math.PI,0.5*Math.PI,true)
        context.fill()

        context.beginPath()
        context.arc(710,275,50,1.5*Math.PI,0*Math.PI,true)
        context.fill()

        context.beginPath()
        context.arc(820,275,50,0*Math.PI,2*Math.PI,false)
        context.fill()
    </script>
</html>
